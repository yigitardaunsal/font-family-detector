{
	"manifest_version": 3,
	"name": "Font Family Detector",
	"version": "1.1",
	"description": "Lists all font-family values on the page",
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icons/icon16.png",
			"48": "icons/icon48.png",
			"128": "icons/icon128.png"
		}
	},
	"permissions": ["activeTab", "scripting"],
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
